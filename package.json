{
  "name": "my-repositories",
  "displayName": "My Repositories",
  "description": "An extension that opens my Github Repo Page",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "my-repositories.openRepoPage",
          "group": "navigation",
          "when": "editorTextFocus"
        }
      ]
    },
    "commands": [
      {
        "command": "my-repositories.openRepoPage",
        "title": "Open My GitHub Repository Page"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "octokit": "^5.0.5"
  }
}
